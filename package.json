    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build -p",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --write \"**/*.{js,jsx,json,md,html,css,less,scss}\"",
        "test": "react-scripts test",
        "test:mocha": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register src/test/**/*.test.ts",
        "test:jest": "env-cmd -f .env.development jest",
        "eject": "react-scripts eject",
        "start:development": "env-cmd -f .env.development yarn start",
        "start:staging": "env-cmd -f .env.staging yarn start",
        "build:staging": "env-cmd -f .env.staging yarn build",
        "build:qa": "env-cmd -f .env.qa yarn build",
        "build:production": "env-cmd -f .env.production yarn build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "babelOptions": {
            "presets": [
                "@babel/preset-react"
            ],
            "caller": {
                "supportsTopLevelAwait": true
            }
        },
        "parser": "@babel/eslint-parser",
        "parserOptions": {
            "requireConfigFile": false,
            "ecmaVersion": 2020,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true,
                "experimentalObjectRestSpread": true
            }
        },
        "env": {
            "browser": true,
            "es6": true
        },
        "globals": {
            "$": true
        },
        "settings": {
            "react": {
                "version": "^17.0.1"
            }
        },
        "rules": {
            "semi": [
                "error",
                "never"
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "moment": "2.29.1",
        "react-error-overlay": "6.0.9"
    },
    "devDependencies": {
        "@truffle/hdwallet-provider": "^2.0.3",
        "babel-eslint": "^10.1.0",
        "eslint-config-standard-react": "^9.2.0",
        "eslint-plugin-eslint-plugin": "^4.1.0",
        "eslint-plugin-react": "^7.20.6",
        "prettier": "^2.1.1"
    },
